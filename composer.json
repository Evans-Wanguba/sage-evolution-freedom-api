{
    "name": "evans-wanguba/sage-evolution-freedom-api",
    "keywords": ["laravel sage", "sage evolution", "freedom api"],
    "description": "This is a PHP package for handling Sage Evolution Freedom web service integration",
    "license": "MIT",
    "authors": [
        {
            "name": "Evans Wanguba",
            "email": "ewanguba@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "~6.0|~7.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.3.1",
        "phpunit/phpunit": "~9.3"
    },
    "autoload": {
        "psr-4": {
            "EvansWanguba\\Sage\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        },
        "laravel": {
            "providers": [
                "EvansWanguba\\Sage\\SageEvolutionFreedomServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
